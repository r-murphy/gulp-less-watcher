{
  "name": "gulp-less-watcher",
  "version": "0.5.0",
  "description": "Watches Less files (and imports) using an endless streaming interface (and generates sourcemaps)",
  "main": "lib/index.js",
  "keywords": [
    "gulpplugin",
    "gulp",
    "less",
    "watch",
    "sourcemap"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/skyrpex/gulp-less-watcher.git"
  },
  "scripts": {
    "clean": "rimraf lib/ && mkdirp lib/",
    "build": "babel src/ --out-dir lib/",
    "watch": "npm run build -- --watch",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "accord": "^0.20.3",
    "deepmerge": "^0.2.10",
    "gaze": "^0.5.2",
    "gulp-util": "^3.0.7",
    "less": "^2.5.3",
    "through2": "^2.0.0",
    "vinyl": "^1.1.0",
    "vinyl-sourcemaps-apply": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.3"
  },
  "author": "Cristian Pallar√©s <me@cpallares.com> (www.cpallares.com)",
  "license": "ISC"
}
